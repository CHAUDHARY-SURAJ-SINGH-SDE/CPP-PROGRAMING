/*for loop repeate the block of code until the condition(defined) will satisfied
in for loop we know how many time's will the loop repeat*/
#include<iostream>
using namespace std;
int main(int argc, char const *argv[])
{
    int i;
    for ( i = 0; i <=10; i++)
    {
        cout<<i<<"\t";
    }
    
    return 0;
}
